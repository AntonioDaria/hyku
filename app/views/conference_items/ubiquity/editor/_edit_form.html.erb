
<% array_of_hash = get_model(curation_concern.class, curation_concern.id, 'editor', 'editor_position') %>


<% if (array_of_hash.class == Array && array_of_hash.first.class == Hash)  %>
  <%= render "conference_items/ubiquity/editor/edit_array_hash_form", array_of_hash: array_of_hash %>
<% elsif array_of_hash.class == Array %>
  <%= render "conference_items/ubiquity/editor/new_form", array: array_of_hash %>
<% elsif array_of_hash.class == Hash %>
  <%= render "conference_items/ubiquity/editor/edit_hash_form", hash: array_of_hash %>
<% else %>
  <%= render "conference_items/ubiquity/editor/new_form", hash: array_of_hash %>
<% end %> <%# closes if array_of_hash.class %>
