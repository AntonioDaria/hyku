
<% array_of_hash = get_model(curation_concern.class, curation_concern.id, 'contributor', 'contributor_position') %>

<% if (array_of_hash.class == Array && array_of_hash.first.class == Hash)  %>
  <%= render "generic_works/contributor/edit_array_hash_form", array_of_hash: array_of_hash %>
<% elsif array_of_hash.class == Array %>
  <%= render "generic_works/contributor/new_form", array: array_of_hash %>
<% elsif array_of_hash.class == Hash %>
  <%= render "generic_works/contributor/edit_hash_form", hash: array_of_hash %>
<% else %>
  <%= render "generic_works/contributor/new_form", hash: array_of_hash %>
<% end %> <%# closes if array_of_hash.class %>
