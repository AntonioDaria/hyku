<% array_of_hash = get_model(presenter.model, presenter.id, 'contributor', 'contributor_position') %>

 <% if (array_of_hash.class == Array && array_of_hash.first.class == Hash )  %>

   <%= render "shared/ubiquity/contributor/show_array_hash", array_of_hash: array_of_hash, source: source   %>

<% elsif array_of_hash.class == Array %>
    <%= render "shared/ubiquity/contributor/show_array", array_of_hash: array_of_hash, source: source   %>

<% elsif array_of_hash.class == Hash %>
    <%= render "shared/ubiquity/contributor/show_hash", array_of_hash: array_of_hash, source: source   %>
<% else %>

<% end %> <%# closes if /else array_of_hash.class %>
