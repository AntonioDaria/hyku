
<% array_of_hash = get_model(curation_concern.class, curation_concern.id, 'creator', 'creator_position') %>
<% if (array_of_hash.class == Array && array_of_hash.first.class == Hash)  %>
  <%= render "shared/ubiquity/creator/edit_form_fields", array_of_hash: array_of_hash %>
<% elsif array_of_hash.class == Array %>

  <%= render "shared/ubiquity/creator/edit_form_array_field", array: array_of_hash %>

<% else %>
<%= render "shared/ubiquity/creator/edit_form_hash_field", hash: array_of_hash %>


<% end %> <%# closes if array_of_hash.class %>
