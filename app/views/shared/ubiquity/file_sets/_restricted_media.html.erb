<!--  Hyku::ManifestEnabledWorkShowPresenter -->
<!-- This whole file was add ubiquitypress to selectively display universalviewer-->
<% if presenter.representative_presenter.image? %>
  <% if zipped_types.include? file_set_presenter.file_format %>
    <!-- dont' displays universal viewer for files with archived format eg zip -->
  <% elsif check_file_is_restricted?(file_set_presenter) && presenter.representative_presenter.image? && (not zipped_types.include? file_set_presenter.file_format) %>
    <%= UniversalViewer.script_tag %>
    <div class="viewer-wrapper"><div class="uv viewer" data-uri="<%= main_app.polymorphic_path [main_app, :manifest, presenter] %>"></div></div>
  <% elsif ( (not file_set_presenter.lease_expiration_date.present?) && (not file_set_presenter.embargo_release_date.present?)  ) %>
    <%= UniversalViewer.script_tag %>
    <div class="viewer-wrapper"><div class="uv viewer" data-uri="<%= main_app.polymorphic_path [main_app, :manifest, presenter] %>"></div></div>
  <% else %>
    <%= media_display presenter.representative_presenter %>
  <% end %>
<% end %>
